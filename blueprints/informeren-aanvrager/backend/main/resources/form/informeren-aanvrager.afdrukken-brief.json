{
    "display": "form",
    "components": [
        {
            "label": "HTML",
            "tag": "p",
            "content": "Het document is gegenereerd. Ga naar tabblad 'Documenten' om het document te openen en af te drukken.",
            "refreshOnChange": false,
            "key": "html",
            "type": "htmlelement",
            "input": false,
            "tableView": false
        },
        {
            "type": "button",
            "label": "Doorgaan",
            "action": "custom",
            "showValidations": false,
            "disableOnInvalid": true,
            "tableView": false,
            "key": "submit",
            "input": true,
            "custom": "form.submit(); component.disabled = true;"
        }
    ]
}